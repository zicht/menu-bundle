{
    "name": "zicht/menu-bundle",
    "description": "Menu utilities for Symfony",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Zicht online",
            "email": "info@zicht.nl"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "gedmo/doctrine-extensions": "^3",
        "knplabs/knp-menu-bundle": "^3.0",
        "symfony/console": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/http-kernel": "^4.4",
        "symfony/security-core": "^4.4",
        "twig/twig": "^2.7 || ^3",
        "zicht/admin-bundle": "^6 || ^7",
        "zicht/framework-extra-bundle": "^9.0",
        "zicht/url-bundle": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Zicht\\Bundle\\MenuBundle\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZichtTest\\Bundle\\MenuBundle\\": [
                "tests/"
            ]
        }
    },
    "minimum-stability": "stable",
    "license": "MIT",
    "scripts": {
        "lint": "php-cs-fixer fix --dry-run --diff -vvv",
        "lint-fix": "php-cs-fixer fix",
        "test": "phpunit"
    }
}
