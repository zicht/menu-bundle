{
    "name": "zicht/menu-bundle",
    "description": "Menu utilities for Symfony",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Zicht online",
            "email": "info@zicht.nl"
        }
    ],
    "require": {
        "php": "^7.1",
        "gedmo/doctrine-extensions": "^2.3",
        "knplabs/knp-menu-bundle": "^3",
        "symfony/console": "^4.4",
        "zicht/admin-bundle": "^6.2.1",
        "zicht/framework-extra-bundle": "^9",
        "zicht/url-bundle": "^5"
    },
    "suggest": {
        "zicht/admin-bundle": "To integrate with Sonata Admin"
    },
    "require-dev": {
        "phpunit/phpunit": "^4"
    },
    "autoload": {
        "psr-4": {
            "Zicht\\Bundle\\MenuBundle\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZichtTest\\Bundle\\MenuBundle\\": [
                "tests/"
            ]
        }
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
